<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- The Application label that appears in app launcher -->
    <string name="application_label">Car Notification Center</string>
    <!-- The label for the NotificationListenerService -->
    <string name="notification_service_label">Car Notification Listener Service</string>
    <!-- The title displayed in notification center activity -->
    <string name="notifications">Notifications</string>
    <string name="expand">Expand</string>
    <string name="collapse">Collapse</string>
    <!-- The text used to display child notification count for group notifications -->
    <plurals name="group_notification_summary_text">
        <item quantity="one"><xliff:g example="1" id="count">%d</xliff:g> Notification</item>
        <item quantity="other"><xliff:g example="2" id="count">%d</xliff:g> Notifications</item>
    </plurals>

</resources>
